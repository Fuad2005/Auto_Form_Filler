{
    "manifest_version": 2,
    "name": "Auto Form Filler",
    "version": "1.0",
    "description": "Fill out forms automatically",
    "background": {
      "scripts": ["background.js"]
    },
    "permissions": ["activeTab", "storage"],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]

  }